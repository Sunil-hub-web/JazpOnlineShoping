<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jazponlineshoping">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JazpOnlineShoping">
        <activity
            android:name=".activity.ProductDetailsActivity"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar"></activity>
        <activity
            android:name=".activity.ProductDescription"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar">

        </activity>
        <activity
            android:name=".activity.Cartpage"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar" />
        <activity
            android:name=".activity.SearchPage"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar" />
        <activity
            android:name=".activity.CategoryPage"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar" />
        <activity
            android:name=".activity.OTPVerifactionPage"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar" />
        <activity
            android:name=".activity.LoginPage"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar" />
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/Theme.JazpOnlineShoping.NoActionBar">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>